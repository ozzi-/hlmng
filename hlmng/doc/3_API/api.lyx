\begin_layout Plain Layout

	"text": "{ 
\backslash
"votingID
\backslash
": 0 , 
\backslash
"name
\backslash
": 
\backslash
"TEST
\backslash
" }",
\end_layout

\begin_layout Plain Layout

	"time": "16:24:55",
\end_layout

\begin_layout Plain Layout

	"title": "presentation_end"
\end_layout

\begin_layout Plain Layout

}
\end_layout

